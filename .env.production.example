# Production Environment Variables
# Copy this file to .env.production and fill in your actual values

# Required - Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Optional - Error Monitoring (Sentry, LogRocket, etc.)
VITE_ERROR_MONITORING_ENDPOINT=https://your-error-monitoring-endpoint.com/api/errors

# Optional - Analytics
VITE_ANALYTICS_ID=your-analytics-id
VITE_ENABLE_ANALYTICS=true

# Optional - Features
VITE_ENABLE_CHAT_SUPPORT=true

# Node Environment (automatically set by build process)
NODE_ENV=production
